{
  "name": "js-wiktionary-scraper",
  "version": "1.3.1",
  "description": "An HTML to JSON wiktionary scraper. It returns the available data for a specified word that is manifested on en.wiktionary.org.",
  "type": "module",
  "main": "index.cjs",
  "module": "index.js",
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs"
  },
  "scripts": {
    "tsc-es6": "tsc --project es6.tsconfig.json",
    "tsc-cjs": "tsc --project cjs.tsconfig.json",
    "package": "python3 package.py"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZerobyteHD/js-wiktionary-scraper.git"
  },
  "keywords": [
    "Scraping",
    "JSON",
    "API",
    "wiktionary",
    "scraper",
    "wiki",
    "word",
    "data"
  ],
  "author": "zerobyte",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZerobyteHD/js-wiktionary-scraper/issues"
  },
  "homepage": "https://github.com/ZerobyteHD/js-wiktionary-scraper#readme",
  "dependencies": {
    "jsdom": "^19.0.0",
    "node-fetch": "^3.2.3",
    "node-fetch2": "npm:node-fetch@^2.6.7"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.14",
    "@types/node-fetch": "^2.6.1"
  }
}
